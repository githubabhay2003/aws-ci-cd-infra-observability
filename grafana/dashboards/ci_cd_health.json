        "h": 6,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "datasource": "Prometheus",
      "targets": [
        {
          "refId": "A",
          "expr": "up{job=\"jenkins\"}",
          "legendFormat": "Jenkins",
          "interval": ""
